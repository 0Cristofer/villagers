{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 6,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": false,
      "RequireLowercase": false
    },
    "User": {
      "RequireUniqueEmail": false
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Cors": {
    "AllowedOrigins": ["https://villagers.com"]
  },
  "Jwt": {
    "Issuer": "villagers-api",
    "Audience": "villagers-game", 
    "Expiration": "1.00:00:00",
    "SecretKey": ""
  }
}
